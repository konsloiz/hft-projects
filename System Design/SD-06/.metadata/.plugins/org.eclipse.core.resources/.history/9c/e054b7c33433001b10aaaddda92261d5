package function_declaration;

import java.util.function.IntUnaryOperator;

public class Main {
	
	public static void main(String[] args) {
	
	IntUnaryOperator factorial = n -> {
		int n_factorial = n*n-1;
		return n_factorial;
	};
	
	int n = 2;
	int n_factorial = factorial.applyAsInt(n);
	
	
	System.out.println(n);
	System.out.println(n_factorial);

	}
}

